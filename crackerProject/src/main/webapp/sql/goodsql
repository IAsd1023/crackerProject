좋아요 SQL

create table GOOD(
GOODNO NUMBER NOT NULL PRIMARY KEY ,
SEQ NUMBER,
MEMBEREMAIL VARCHAR2(100) NOT NULL,
GOODCHECK NUMBER DEFAULT 0 NOT NULL,
GOODDATE DATE DEFAULT SYSDATE NOT NULL,
FOREIGN KEY (MEMBEREMAIL) REFERENCES CRACKERMEMBER (MEMBEREMAIL) ON DELETE CASCADE,
FOREIGN KEY (SEQ) REFERENCES BOARD (SEQ) ON DELETE CASCADE,
FOREIGN KEY (SEQ) REFERENCES RECIPEBOARD (SEQ) ON DELETE CASCADE
);